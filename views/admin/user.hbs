<h1>後台頁面</h1>
<br>
<hr />

<div class="container">
  <h3>使用者名單</h3>
  <table class="table">
    <thead class="table-danger">
      <tr>
        <th>使用者名稱:</th>
        <th>email:</th>
        <th>是否為admin:</th>
      </tr>
    </thead>
    <tbody>
      {{#each users}}
      <tr>
        <td>{{this.name}}</td>
        <td>{{this.email}}</td>
        <td>{{this.isAdmin}}</td>
      </tr>
      {{/each }}
    </tbody>
  </table>
</div>